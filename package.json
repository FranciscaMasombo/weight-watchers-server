{
  "name": "weight-watchers-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm-run-all test clean",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/submissions-model-test.js",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "server": "node lib/bin/www",
    "clean": "rimraf ./lib && mkdir lib",
    "start": "npm run test && npm run build && npm run server",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test:watch": "npm run test -- --watch",
    "lint": "esw  ./test ./routes ./models/ app.js",
    "lint:watch": "npm run lint -- --watch",
    "postbuild": "npm run server"
  },
  "dependencies": {
    "growl": "1.10.0",
    "superagent": "3.7.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.16",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-google": "^0.11.0",
    "eslint-watch": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "ws": "3.3.2"
  }
}
